    "main": "index.js",
    "scripts": {
        "start": "yarn build && node dist/index.js",
        "start:watch": "nodemon --delay 150ms --exitcrash --watch dist --ext js,ts --exec node --no-warnings dist/index.js --color",
        "dev": "nodemon --exitcrash --watch src --ext js,ts --exec node --no-warnings --loader ts-node/esm src/index.ts --color",
        "build": "tsc",
        "build:watch": "tsc -w",
        "build:test": "tsc --noEmit",
        "format": "prettier --write --config .prettierrc \"src/**/*.ts\"",
        "check-updates": "ncu",
        "check-updates:upgrade": "ncu -u --filterVersion '^*'"
